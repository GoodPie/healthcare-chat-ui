{
  "name": "@healthcare-chat/registry",
  "version": "0.1.0",
  "private": true,
  "types": "./dist/index.d.ts",
  "main": "./dist/index.js",
  "bin": "./dist/bin/index.js",
  "scripts": {
    "build": "tsup src/index.ts --format esm,cjs --dts && tsup bin/registration.ts --format cjs --no-dts --out-dir dist/bin",
