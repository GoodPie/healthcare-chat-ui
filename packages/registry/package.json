    "lint": "eslint .",
    "test": "jest --passWithNoTests"
  },
  "dependencies": {
    "@healthcare-chat/core": "workspace:*",
    "@healthcare-chat/tools": "workspace:*",
    "commander": "^12.0.0",
    "fs-extra": "^11.2.0",
    "zod": "^3.25.28"
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/node": "^22.15.21",
    "eslint": "^9.27.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.3.4",
    "tsup": "^8.0.1",
    "typescript": "^5.8.3"
  }
}
