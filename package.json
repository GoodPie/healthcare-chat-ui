{
  "name": "healthcare-chat-ui-kit",
  "version": "0.1.0",
  "private": true,
  "packageManager": "yarn@4.9.1",
  "engines": {
    "node": ">=18.0.0",
    "yarn": ">=4.9.1"
  },
  "workspaces": [
    "packages/*",
    "apps/*",
    "docs"
  ],
  "scripts": {
    "build": "yarn workspaces foreach --all -p run build",
    "build:packages": "yarn workspaces foreach --recursive --from './packages/*' run build",
    "build:docs": "yarn workspace docs run build",
    "dev": "yarn workspaces foreach --all -p run dev",
    "lint": "yarn workspaces foreach --all -p run lint",
    "test": "yarn workspaces foreach --all -p run test"
  },
  "devDependencies": {
    "@eslint/css": "^0.8.1",
    "@eslint/js": "^9.27.0",
    "@eslint/json": "^0.12.0",
    "@types/fs-extra": "^11.0.4",
    "@types/jsonfile": "^6.1.4",
    "@types/node": "^22.15.26",
    "eslint": "^9.27.0",
    "eslint-plugin-react": "^7.37.5",
    "events": "^3.3.0",
    "fs-extra": "^11.3.0",
    "globals": "^16.2.0",
    "jest": "^29.7.0",
    "punycode": "^2.3.1",
    "string_decoder": "^1.3.0",
    "ts-jest": "^29.3.4",
    "tsup": "^8.5.0",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.32.1",
    "util": "^0.12.5"
  },
  "license": "MIT",
  "dependencies": {
    "commander": "^14.0.0",
    "esprima": "^4.0.1",
    "jsonfile": "^6.1.0",
    "postcss": "^8.5.3",
    "react": "^19.1.0",
    "supports-color": "^8.1.1",
    "tailwindcss": "^4.1.7",
    "zod": "^3.25.28"
  },
  "resolutions": {
    "commander": "^14.0.0",
    "eslint": "^9.27.0",
    "tsup": "^8.5.0",
    "jsonfile": "^6.1.0",
    "@types/node": "^22.15.26",
    "@types/jsonfile": "^6.1.4",
    "@types/fs-extra": "^11.0.4",
    "fs-extra": "^11.3.0",
    "util": "^0.12.5",
    "punycode": "^2.3.1",
    "events": "^3.3.0",
    "string_decoder": "^1.3.0",
    "tailwindcss": "^4.1.7",
    "react": "^19.1.0",
    "@eslint/css": "^0.8.1",
    "@eslint/js": "^9.27.0",
    "@eslint/json": "^0.12.0"
  }
}
